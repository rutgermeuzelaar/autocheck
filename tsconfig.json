{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
        "ES2021",
        "DOM",
        "DOM.Iterable"
    ],
    "typeRoots": [
      "./types"
    ]
  },
  "display": "Recommended",
  "$schema": "https://json.schemastore.org/tsconfig"
}